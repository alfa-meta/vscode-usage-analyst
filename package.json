{
  "name": "vscode-usage-analyst",
  "displayName": "VSCode Usage Analyst",
  "description": "Tracks your usage of VS Code",
  "version": "0.1.9",
  "publisher": "alfa-meta",
  "icon": "resources/icon.png",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "activationEvents": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/alfa-meta/vscode-usage-analyst.git"
  },
  "changelog": "https://github.com/alfa-meta/vscode-usage-analyst/main/CHANGELOG.md",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "usageAnalyst",
          "title": "Usage Analyst",
          "icon": "resources/icon.png"
        }
      ]
    },
    "views": {
      "usageAnalyst": [
        {
          "id": "usageOverview",
          "name": "Overview"
        }
      ]
    },
    "commands": [
      {
        "command": "vscode-usage-analyst.analyst",
        "title": "Show VSCode Usage"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./",
    "vscode:prepublish": "tsc -p ./"
  },
  "devDependencies": {
    "@types/node": "^18.19.74",
    "@types/pidusage": "^2.0.5",
    "@types/vscode": "^1.80.0",
    "install": "^0.13.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "pidusage": "^3.0.2"
  }
}
