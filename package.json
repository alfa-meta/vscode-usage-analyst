{
  "name": "vscode-usage-analyst",
  "displayName": "VSCode Usage Analyst",
  "description": "Tracks your usage of VS Code",
  "version": "0.1.0",
  "publisher": "alfa-meta",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "activationEvents": [
    
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "usageAnalyst",
          "title": "Usage Analyst",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "usageAnalyst": [
        {
          "id": "usageOverview",
          "name": "Overview"
        }
      ]
    },
    "commands": [
      {
        "command": "vscode-usage-analyst.analyst",
        "title": "Show VSCode Usage"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./",
    "vscode:prepublish": "tsc -p ./"
  },
  "devDependencies": {
    "@types/node": "^18.20.6",
    "@types/vscode": "^1.80.0",
    "install": "^0.13.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "keypress": "^0.2.1",
    "nvm": "^0.0.4"
  }
}
